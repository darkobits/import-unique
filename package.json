{
  "name": "@darkobits/import-unique",
  "version": "1.0.2",
  "description": "Import an un-cached version of a module.",
  "license": "WTFPL",
  "contributors": [
    {
      "name": "Joshua Martin",
      "email": "joshua@frontlawn.net"
    }
  ],
  "keywords": [
    "import",
    "require",
    "cache",
    "nocache"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkobits/import-unique.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/import-unique.js",
  "scripts": {
    "clean": "del dist",
    "check:styles": "tslint --project tsconfig.json --format codeFrame",
    "check:types": "tsc --emitDeclarationOnly && rimraf **/*.spec.d.ts",
    "test": "mocha --require ./etc/test-setup.js --recursive src/**/*.spec.ts",
    "prebuild": "run-p clean check:*",
    "build": "babel src --extensions='.ts' --ignore='**/*.spec.ts,**/*.d.ts' --out-dir=dist --source-maps=inline --verbose",
    "prebump": "run-p test build",
    "bump": "standard-version",
    "bump:beta": "npm run prebump && standard-version --prerelease=beta",
    "link": "npm run build -- --out-dir=$(npx clean-link-dir)/dist --watch --verbose"
  },
  "dependencies": {
    "caller-path": "^2.0.0",
    "resolve-from": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@darkobits/clean-link": "^2.0.1",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.9.4",
    "@types/resolve-from": "0.0.18",
    "babel-core": "^7.0.0-bridge.0",
    "chai": "^4.1.2",
    "del-cli": "^1.1.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.9",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "tslint-xo": "^0.9.0",
    "typescript": "^3.0.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "exclude": [
      "coverage",
      "dist",
      "etc",
      "mocks",
      "**/*.spec.*",
      "**/*.d.ts"
    ],
    "all": true
  }
}
